.page-container {
  padding: 20rpx;
  background-color: #f5f5f5;
}

.reservation-notice-container {
  background-color: #ffffff;
  border-radius: 16rpx;
  padding: 30rpx;
  box-shadow: 0 2rpx 10rpx rgba(0, 0, 0, 0.1);
}

.notice-content {
  font-size: 28rpx;
  color: #666666;
}

.notice-item {
  display: block;
  margin-bottom: 15rpx;
  line-height: 1.6;
}

.notice-content text {
  display: block;
  margin-bottom: 10px; /* 每个须知项之间的间距 */
}